    <!--
        ISSUE:

        These styles have to be here, and they have to be Static Resources,
        because the InfoExtension uses a WinForms custom control to which we
        "apply" WPF styles.
    -->

    <Style x:Key="InfoExtension_DataGridColumnHeaderStyle" TargetType="{x:Type DataGridColumnHeader}">
        <Setter Property="Height" Value="25"/>
        <Setter Property="Background" Value="{StaticResource InfoExtension_DataGridColumnHeaderBackgroundBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource InfoExtension_DataGridColumnHeaderForegroundBrush}"/>
    </Style>

    <Style x:Key="InfoExtension_DataGridCellStyle" TargetType="{x:Type DataGridCell}">
        <Style.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="#807FFF00"/>
            </Trigger>

            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#50800080"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="InfoExtension_DataGridRowStyle" TargetType="{x:Type DataGridRow}">
        <Style.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="#3200CED1"/>
            </Trigger>

            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#8087CEEB"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="InfoExtension_DataGridStyle" TargetType="{x:Type DataGrid}">
        <Setter Property="Background" Value="{StaticResource InfoExtension_DataGridBackgroundBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource InfoExtension_DataGridForegroundBrush}"/>
        <Setter Property="FontSize" Value="{StaticResource InfoExtension_DataGridFontSize}"/>
        <Setter Property="ColumnHeaderStyle" Value="{StaticResource InfoExtension_DataGridColumnHeaderStyle}"/>
        <Setter Property="CellStyle" Value="{StaticResource InfoExtension_DataGridCellStyle}"/>
        <Setter Property="RowStyle" Value="{StaticResource InfoExtension_DataGridRowStyle}"/>
    </Style>


